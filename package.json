{
    "name": "smash-nab-demo",
    "version": "0.0.0-alpha",
    "description": "Smash Neighborhood Admin Bot Demo",
    "type": "module",
    "main": "dist/src/index.js",
    "scripts": {
        "postinstall": "patch-package",
        "prepare": "husky",
        "start": "node .",
        "dev": "nodemon --exec tsx --ignore './scripts/*' ./src/index.ts",
        "test": "jest --config jest.config.cjs --coverage",
        "test:w": "npm run test -- --watchAll",
        "build": "rm -rf dist/* && tsc -p tsconfig.build.json && tsc-alias",
        "build:checks": "npm run prettier:check && npm run build && npm run eslint-dist:check",
        "eslint-dist:check": "eslint dist/**/*.js",
        "prettier": "prettier --config .prettierrc 'src/**/*.ts'",
        "prettier:check": "npm run prettier -- --check",
        "prettier:format": "npm run prettier -- --write",
        "script/auto": "tsx ./scripts/automated-client.ts",
        "script/client": "tsx ./scripts/client.ts",
        "script/json": "tsx ./scripts/create-identity.ts",
        "script/id": "tsx ./scripts/generate-identity-hsm.ts",
        "script/sign": "tsx ./scripts/test-sign.ts",
        "create-id": "npm run --silent script/json 0 1 0 > ./out/identity-exported.json",
        "generate-id": "mkdir -p config && npm run --silent script/id 0 1 0 > ./config/identity.json",
        "docker:build": "docker build . -t smash-nbh-bot",
        "docker:test": "docker build . -t softhsmv2 && docker build . -f Dockerfile.test"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "cytoscape": "^3.29.2",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "graphene-pk11": "^2.3.6",
        "node-webcrypto-p11": "^2.6.6",
        "smash-node-lib": "./smash-node-lib/"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/cytoscape": "^3.21.2",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.13",
        "eslint": "^9.9.1",
        "husky": "^9.1.6",
        "jest": "^29.7.0",
        "nodemon": "^3.1.7",
        "patch-package": "^8.0.0",
        "prettier": "^3.3.3",
        "ts-jest": "^29.2.5",
        "tsc-alias": "^1.8.10",
        "tsx": "^4.19.1",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.16.0"
    }
}
